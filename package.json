{
  "name": "@subspace-chat/sdk",
  "version": "0.0.1",
  "description": "A TypeScript client for the Subspace decentralized chat platform on AO",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./profile": {
      "import": "./dist/esm/profile.js",
      "require": "./dist/cjs/profile.js",
      "types": "./dist/types/profile.d.ts"
    },
    "./server": {
      "import": "./dist/esm/server.js",
      "require": "./dist/cjs/server.js",
      "types": "./dist/types/server.d.ts"
    },
    "./bot": {
      "import": "./dist/esm/bot.js",
      "require": "./dist/cjs/bot.js",
      "types": "./dist/types/bot.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run postbuild",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:watch": "tsc -p tsconfig.esm.json --watch",
    "clean": "rm -rf dist",
    "postbuild": "node scripts/postbuild.js",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "arweave",
    "ao",
    "chat",
    "decentralized",
    "messaging",
    "subspace",
    "typescript"
  ],
  "author": "Ankush Singh <ankush4singh@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ankushKun/subspace-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ankushKun/subspace-sdk/issues"
  },
  "homepage": "https://github.com/ankushKun/subspace-lib#readme",
  "peerDependencies": {
    "@permaweb/aoconnect": "^0.0.50"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}