{
  "name": "@subspace-protocol/sdk",
  "version": "0.0.2",
  "description": "A TypeScript client for the Subspace decentralized chat platform on AO",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run postbuild",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:watch": "tsc -p tsconfig.esm.json --watch",
    "clean": "rm -rf dist",
    "postbuild": "node scripts/postbuild.js",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch ",
    "typecheck": "tsc --noEmit",
    "publish-npm": "npm run build && npm publish --access public"
  },
  "keywords": [
    "arweave",
    "ao",
    "chat",
    "decentralized",
    "messaging",
    "subspace",
    "typescript"
  ],
  "author": "Ankush Singh <ankush@arweave.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/subspace-dev/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/subspace-dev/sdk/issues"
  },
  "homepage": "https://github.com/subspace-dev/sdk#readme",
  "peerDependencies": {
    "@permaweb/aoconnect": "^0.0.85"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}